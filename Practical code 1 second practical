#include<iostream>
using namespace std;

int main(){

int p1,p2;
cout << "enter two integers" << endl;
cin >> p1>>p2;
int sum=p1+p2;
cout<<p1<<"+"<<p2<<"="<<sum<<endl;
int difference=p1-p2;
cout<<p1<<"-"<<p2<<"="<<difference<<endl;
int multi=p1*p2;
cout<<p1<<"x"<<p2<<"="<<multi<<endl;
int div=p1/p2;
cout<<p1<<"รท"<<p2<<"="<<div<<endl;

}
